    </rule>
    <rule id="8a586f8a-e228-4c31-1699-c8c837d2b0fd" name="Tank Hunters" page="0" hidden="false">
      <description>No Tank Fear test when assaulting Tanks if Anti-tank Grenades have been taken</description>
    </rule>
    <rule id="4808ac5e-bfa8-007f-ba2e-14927313cb81" name="Tankette" page="0" hidden="false">
