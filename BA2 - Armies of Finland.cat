        <rule id="d9f23297-40c6-518d-7d32-1aaba9b62d6d" name="Armoured all round" page="0" hidden="false">
                  
            <description>no modifiers apply for penetration when shooting at sides, rear or from above.</description>
                
        </rule>
            
        <rule id="b1b4eeee-8721-b558-3729-9018a7a70030" name="Armoured Cab" page="0" hidden="false">
                  
            <description>Damage Value 7+</description>
                
        </rule>
            
        <rule id="35773851-b4ba-791a-3aad-49965b6468a8" name="Armoured Car" page="0" hidden="false">
                  
            <description>Damage Value 7+</description>
                
        </rule>
            
        <rule id="c1b7a00c-0601-30e7-cfc3-0f2bb186d970" name="Armoured Carrier" page="0" hidden="false">
                  
