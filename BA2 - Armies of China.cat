        <profile id="2b905c71-6ad1-3538-08a6-5cd28e730189" name="Flamethrower (Vehicle)" page="0" hidden="false" typeId="58b84a31-b571-5b2b-0a61-a8b39a74f6eb" typeName="Weapon">
                  
            <characteristics>
                        
                <characteristic name="Range" typeId="d527d332-6869-32b4-3a5a-13127d8e04c9">18"</characteristic>
                        
