    <profile id="be1b99c6-2ede-c8ec-f101-5deced71a400" name="Flamethrower (Infantry)" page="0" hidden="false" typeId="58b84a31-b571-5b2b-0a61-a8b39a74f6eb" typeName="Weapon">
