    <rule id="992c40be-89ca-5664-e796-cf1397c3eea0" name="Multiple Rocket Launcher with 360 Degree arc of fire" page="0" hidden="false"/>
    <rule id="ada08d75-5333-da8a-628d-f12f0d2a5ba0" name="Nebelwerfer (Heavy Mortar)" page="0" hidden="false"/>
