    <rule id="5916e308-c470-afef-a4bc-76ccbfca2185" name="Platform-mounted Light AT Gun with 360 defree arc of fire" page="0" hidden="false"/>
    <rule id="bbe7fe4c-6faa-446a-43a5-70dbba55b2d0" name="Platform-mounted Light Autocannon with 360 defree arc of fire" page="0" hidden="false"/>
    <rule id="15cd7c8e-6131-95ad-29d8-1fb0a00c2638" name="Platform-mounted Medium AT Gun with 360 defree arc of fire" page="0" hidden="false"/>
