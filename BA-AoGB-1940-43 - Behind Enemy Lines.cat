    <rule id="8e020423-0d92-146a-3748-990ad45584bc" name="Turret-mounted Flamethrower" page="0" hidden="false"/>
    <rule id="0f772bf5-50a6-0b36-102e-a0dd871c8a34" name="Turret-mounted Heavy AT Gun" page="0" hidden="false"/>
    <rule id="b8df9ce1-9549-20bf-2eac-d182e8ce1c20" name="Turret-mounted Heavy Howitzer" page="0" hidden="false"/>
